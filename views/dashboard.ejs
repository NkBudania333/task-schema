<h1>hii <%= name %></h1>

<form action="/tasks/<%= name %>" method="POST">
    <input class="newtask form-control" id="newtask" name="newtask" />
    <button class="btn">Add</button>
    
</form>

<br>
<div class="list">
    <% for (var i =0; i <value.length;  i++ ) { %>
        <p> <%= value[i] %></p> <a href="/edit/<%= name %>/<%= value[i]%>">EDIT</a>&nbsp;&nbsp;<a href="/delete/<%= name %>/<%= value[i] %>">DELETE</a>
    <% } %>
</div>
